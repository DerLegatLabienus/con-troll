{
  "name": "ConTroll",
  "description": "ConTroll Convention Manager Web Service",
  "image": "heroku/php",
  "scripts": {},
  "env": {
    "DB_URL_ENV_NAME": {
      "required": true
    },
    "EMAIL_IMPL": {
      "required": true
    },
    "FACEBOOK_AUTH_CLIENT": {
      "required": true
    },
    "GOOGLE_AUTH_CLIENT": {
      "required": true
    },
    "SMTP_HOST": {
      "required": true
    },
    "SMTP_PASSWORD": {
      "required": true
    },
    "SMTP_PORT": {
      "required": true
    },
    "SMTP_USERNAME": {
      "required": true
    },
    "TWITTER_AUTH_CLIENT": {
      "required": true
    }
  },
  "addons": [
    "cleardb",
    "sparkpost"
  ],
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-php"
    }
  ]
}
